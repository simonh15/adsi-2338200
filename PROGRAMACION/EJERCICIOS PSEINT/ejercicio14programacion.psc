Proceso ejercicio14
	//Pedir el radio de una circunferencia y calcular su longitud.
	
	//definicion de variables
	definir radius Como Real;
	definir lenght Como Real;
	//operacion de entrada 
	escribir "write the radius :" ;
	leer radius;
	
	// procedimiento 
	lenght= 2 * PI * radius;
	
	//Operacio de salida 
	Escribir " the lenght of the circumference is : " , lenght ;
	
FinProceso
